s=\"chart-wrapper\" :style=\"{ height: 46 }\">\r\n      <v-chart :force-fit=\"true\" :height=\"height\" :data=\"data\" :padding=\"[36, 5, 18, 5]\">\r\n        <v-tooltip />\r\n        <v-bar position=\"x*y\" />\r\n      </v-chart>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment'\r\nconst data = []\r\nconst beginDay = new Date().getTime()\r\n\r\nfor (let i = 0; i < 10; i++) {\r\n  data.push({\r\n    x: moment(new Date(beginDay + 1000 * 60 * 60 * 24 * i)).format('YYYY-MM-DD'),\r\n    y: Math.round(Math.random() * 10)\r\n  })\r\n}\r\n\r\nconst tooltip = [\r\n  'x*y',\r\n  (x, y) => ({\r\n    name: x,\r\n    value: y\r\n  })\r\n]\r\n\r\nconst scale = [{\r\n  dataKey: 'x',\r\n  min: 2\r\n}, {\r\n  dataKey: 'y',\r\n  title: '时间',\r\n  min: 1,\r\n  max: 30\r\n}]\r\n\r\nexport default {\r\n  name: 'MiniBar',\r\n  data () {\r\n    return {\r\n      data,\r\n      tooltip,\r\n      scale,\r\n      height: 100\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  @import \"chart\";\r\n</style>\r\n"],"sourceRoot":"src/components/Charts"}]}