mb class=\"file-path\">\r\n\t\t\t\t<a-breadcrumb-item>Home</a-breadcrumb-item>\r\n\t\t\t\t<a-breadcrumb-item><a href=\"\">Application Center</a></a-breadcrumb-item>\r\n\t\t\t\t<a-breadcrumb-item><a href=\"\">Application List</a></a-breadcrumb-item>\r\n\t\t\t\t<a-breadcrumb-item>An Application</a-breadcrumb-item>\r\n\t\t\t</a-breadcrumb>\r\n    </div>\r\n\t\t\r\n\t\t<div class=\"table-operator operator file-operator\">\r\n\t\t  <a-button icon=\"upload\">上传</a-button>\r\n\t\t\t<a-button icon=\"folder-add\">文件夹</a-button>\r\n\t\t\t<a-button type=\"danger\">删除</a-button>\r\n\t\t  <a-dropdown v-if=\"selectedRowKeys.length > 0\">\r\n\t\t    <a-menu slot=\"overlay\">\r\n\t\t      <a-menu-item key=\"1\"><a-icon type=\"delete\" />删除</a-menu-item>\r\n\t\t      <!-- lock | unlock -->\r\n\t\t      <a-menu-item key=\"2\"><a-icon type=\"lock\" />锁定</a-menu-item>\r\n\t\t    </a-menu>\r\n\t\t    <a-button style=\"margin-left: 8px\">\r\n\t\t      更多 <a-icon type=\"down\" />\r\n\t\t    </a-button>\r\n\t\t  </a-dropdown>\r\n\t\t</div>\r\n\r\n    \r\n\r\n\t\t<a-button type=\"primary\" @click=\"start\" :disabled=\"!hasSelected\" :loading=\"loading\">\r\n\t\t\t\t\tReload\r\n\t\t</a-button>\r\n\t\t<span style=\"margin-left: 8px\">\r\n\t\t<template v-if=\"hasSelected\">\r\n\t\t\t{{\t`Selected ${selectedRowKeys.length} items`\t}}\r\n\t\t</template>\r\n\t\t</span>\r\n\t\t<a-table\r\n\t\t\tref=\"fileList\"\r\n\t\t\tsize=\"small\"\r\n      :rowSelection=\"{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}\"\r\n      :columns=\"columns\"\r\n      :dataSource=\"localDataSource\"\r\n\t\t\t:loading=\"loading\"\r\n\t\t\t:pagination=\"{showSizeChanger: true,pageSize: 150,\tpageSizeOptions:\t['150', '200', '500', '1000']}\"\r\n\t\t\t:scroll=\"{ y: 420 }\"\r\n\t\t\t:customRow=\"customRow\"\r\n\t\t\t@change=\"handleTableChange\"\r\n    >\r\n\t\t\t<template\r\n\t\t\t\tv-for=\"col in ['fileName']\"\r\n\t\t\t\t:slot=\"col\"\r\n\t\t\t\tslot-scope=\"text, record\"\r\n\t\t\t>\r\n\t\t\t\t<div :key=\"col\">\r\n\t\t\t\t\t<a-input\r\n\t\t\t\t\t\tv-if=\"record.editable\"\r\n\t\t\t\t\t\tstyle=\"margin: -5px 0\"\r\n\t\t\t\t\t\t:value=\"text\"\r\n\t\t\t\t\t\t@change=\"e => handleChange(e.target.value, record.key, col)\"\r\n\t\t\t\t\t/>\r\n\t\t\t\t\t<template v-else>{{ text }}</template>\r\n\t\t\t\t</div>\r\n\t\t\t</template>\r\n\t\t\t<template slot=\"operation\" slot-scope=\"text, record, index\">\r\n\t\t\t\t<div class=\"editable-row-operations\">\r\n\t\t\t\t\t<span v-if=\"record.editable\">\r\n\t\t\t\t\t\t<a @click=\"() => save(record.key)\">Save</a>\r\n\t\t\t\t\t\t<a-popconfirm title=\"Sure to cancel?\" @confirm=\"() => cancel(record.key)\">\r\n\t\t\t\t\t\t\t<a>Cancel</a>\r\n\t\t\t\t\t\t</a-popconfirm>\r\n\t\t\t\t\t</span>\r\n\t\t\t\t\t<span v-else>\r\n\t\t\t\t\t\t<a @click=\"() => edit(record.key)\">Edit</a>\r\n\t\t\t\t\t</span>\r\n\t\t\t\t</div>\r\n\t\t\t</template>\r\n\t\t\t\r\n\t\t</a-table>\r\n\t\t\t<v-contextmenu ref=\"contextmenu\" @contextmenu=\"handleContextmenu\">\r\n\t\t\t\t<v-contextmenu-item>菜单1</v-contextmenu-item>\r\n\t\t\t\t<v-contextmenu-item>菜单2</v-contextmenu-item>\r\n\t\t\t\t<v-contextmenu-item>菜单3</v-contextmenu-item>\r\n\t\t\t</v-contextmenu>\r\n\t\t</a-card>\r\n\t</template>\r\n\r\n<script>\r\n\r\n\r\nexport default {\r\n  name: 'TableList',\r\n\tmounted ()\t{\r\n\t\tthis.loading = true\r\n\t\tthis.$http.get('/fileList', {\r\n\t\t\tparams: Object.assign(this.queryParam)\r\n\t\t}).then(res => {\r\n\t\t\tthis.localDataSource = res\r\n\t\t\tthis.loading = false\r\n\t\t\t/* this.$nextTick(() => {\r\n        var items = this.$refs.fileList.$el.querySelectorAll('tbody>tr')\r\n\t\t\t\tfor\t(var i = 0; i < items.length; i++)\t{\r\n\t\t\t\t\tconsole.log(items[i])\r\n\t\t\t\t\titems[i].addEventListener('click',\tfunction()\t{\r\n\t\t\t\t\t\tconsole.log('点击')\r\n\t\t\t\t\t\tthat.menuShow()\r\n\t\t\t\t\t})\r\n\t\t\t\t}\r\n      }) */\r\n\t\t})\r\n\t},\r\n  data () {\r\n    return {\r\n      queryParam: {},\r\n\t\t\tcolumns:\t[\r\n\t\t\t\t{\r\n\t\t\t\t\ttitle: '文件名',\r\n\t\t\t\t\tdataIndex: 'fileName',\r\n\t\t\t\t\tscopedSlots: { customRender: 'fileName' }\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\ttitle: '文件大小',\r\n\t\t\t\t\tdataIndex: 'fileSize',\r\n\t\t\t\t\twidth: '120px'\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\ttitle: '修改日期',\r\n\t\t\t\t\tdataIndex: 'updatedAt',\r\n\t\t\t\t\twidth: '180px'\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\ttitle: '操作',\r\n\t\t\t\t\tdataIndex: 'operation',\r\n\t\t\t\t\tscopedSlots: { customRender: 'operation' },\r\n\t\t\t\t\twidth: '120px'\r\n\t\t\t\t}\r\n\t\t\t],\r\n\t\t\tlocalDataSource: [],\r\n\t\t\tselectedRowKeys: [],\r\n\t\t\tloading: true\r\n    }\r\n  },\r\n\tcomputed: {\r\n      hasSelected() {\r\n        return this.selectedRowKeys.length > 0\r\n      }\r\n  },\r\n  methods: {\r\n\t\thandleTableChange() {\r\n      console.log('handleTableChange')\r\n    },\r\n\t\tcustomRow(record, index) {\r\n      return {\r\n        on: {\r\n          click: () => {\r\n            console.log('aaa')\r\n          }\r\n        }\r\n      }\r\n    },\r\n\t\thandleContextmenu (vnode) {\r\n        console.log('被点击区域的 vnode:', vnode)\r\n    },\r\n\t\tmenuShow()\t{\r\n\t\t\tconsole.log(this.$refs.contextmenu)\r\n\t\t\tthis.$refs.contextmenu.show()\r\n\t\t},\r\n\t\tstart() {\r\n        this.loading = true\r\n        // ajax request after empty completing\r\n        setTimeout(() => {\r\n          this.loading = false\r\n          this.selectedRowKeys = []\r\n        }, 1000)\r\n      },\r\n\t\tonSelectChange(selectedRowKeys) {\r\n\t\t\tconsole.log('selectedRowKeys changed: ', selectedRowKeys)\r\n\t\t\tthis.selectedRowKeys = selectedRowKeys\r\n\t\t},\r\n\t\thandleChange(value, key, column) {\r\n\t\t\t console.log(value, key, column)\r\n        const newData = [...this.localDataSource]\r\n        const target = newData.filter(item => key === item.key)[0]\r\n        if (target) {\r\n          target[column] = value\r\n          this.localDataSource = newData\r\n        }\r\n\t\t},\r\n\t\tedit(key) {\r\n        const target = this.localDataSource.filter(item => key === item.key)[0]\r\n        if (target) {\r\n          target.editable = true\r\n        }\r\n\t\t},\r\n\t\tsave(key) {\r\n\t\t\tconst target = this.localDataSource.filter(item => key === item.key)[0]\r\n\t\t\t target.editable = false\r\n\t\t\t},\r\n\t\t\tcancel(key) {\r\n        const target = this.localDataSource.filter(item => key === item.key)[0]\r\n        target.editable = false\r\n\t\t\t}\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\t.editable-row-operations a {\r\n    margin-right: 8px;\r\n  }\r\n  .search {\r\n    margin-bottom: 54px;\r\n  }\r\n\r\n  .fold {\r\n    width: calc(100% - 216px);\r\n    display: inline-block\r\n  }\r\n\r\n  .operator {\r\n    margin-bottom: 18px;\r\n  }\r\n\t\r\n\t.file-operator > .ant-btn{\r\n\t\tmargin-right: 8px;\r\n\t}\r\n\t\r\n\t.file-path-tools {\r\n\t\tposition: absolute;\r\n\t}\r\n\t\r\n\t.file-path-tools button{\r\n\t\tfont-size: 14px;\r\n\t\tborder: 1px solid transparent;\r\n\t}\r\n\t\r\n\t.file-path-tools button:hover {\r\n\t  background-color: #f8f8f8;\r\n\t}\r\n\t\r\n\t.file-path {\r\n\t\tmargin-left: 150px;\r\n\t\tbackground-color: #f1f3f4;\r\n\t\tborder-radius: 14px;\r\n\t\tpadding: 5px 5px 5px 10px;\r\n\t}\r\n\t\r\n\t.file-icon {\r\n\t\tmargin-right: 5px;\r\n\t\tfont-size: 18px\r\n\t}\r\n\t\r\n  @media screen and (max-width: 900px) {\r\n    .fold {\r\n      width: 100%;\r\n    }\r\n  }\r\n</style>\r\n"]}]}